{
  "order" : 0,
  "template" : "{{ prefix }}_*",
  "settings" : {
    "index.refresh_interval" : "5s",
    "analysis": {
      "analyzer": {
        "default": {
          "filter": [
            "standard", 
            "lowercase", 
            "word_delimiter"
          ], 
          "tokenizer": "keyword"
        }
      }
    }
  },
  "mappings" : {
    "_default_" : {
      "dynamic_templates" : [ {
        "string_fields" : {
          "mapping" : {
            "index" : "analyzed",
            "omit_norms" : true,
            "type" : "string",
            "fields" : {
              "raw" : {
                "index" : "not_analyzed",
                "ignore_above" : 256,
                "type" : "string"
              }
            }
          },
          "match_mapping_type" : "string",
          "match" : "*"
        }
      } ],
      "_id": {
        "type": "string",
        "store": true,
        "index": "not_analyzed"
      },
      "_timestamp": {
        "enabled": true,
        "store": true
      },
      "_all" : {
        "enabled" : true
      },
      "properties" : {
        "browse_urls": {
          "index_name": "browse_url",
          "type": "string"
        },
        "urls": {
          "index_name": "url",
          "type": "string"
        },
        "location": {
          "tree": "quadtree",
          "type": "geo_shape",
          "precision": "1m"
        },
        "center": {
          "tree": "quadtree",
          "type": "geo_shape",
          "precision": "1m"
        },
        "starttime": {
          "type": "date",
          "format": "dateOptionalTime"
        },
        "endtime": {
          "type": "date",
          "format": "dateOptionalTime"
        },
        "creation_timestamp": {
          "type": "date",
          "format": "dateOptionalTime"
        },
        "metadata": {
          "properties": {
            "context": {
              "type": "object",
              "enabled": false
            }
          }
        },
        "prov": {
          "properties": {
            "wasDerivedFrom": {
              "index": "not_analyzed",
              "type": "string"
            },
            "wasGeneratedBy": {
              "index": "not_analyzed",
              "type": "string"
            }
          }
        }
      }
    }
  },
  "aliases" : {
    "{{ alias }}" : {}
  }
}
